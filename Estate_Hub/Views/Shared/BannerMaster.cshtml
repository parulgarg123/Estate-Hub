<body class="Signup1">
   
    <!-- Header carousel -->
    <div class="header_main">
        <header data-spy="affix" data-offset-top="100" style="background-color: #4F14AC;" class="inner_header">
            <div class="container">
                <div class="navbar-brand">
                    <button data-target="#navbar" data-toggle="collapse" class="navbar-toggle collapsed">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="logo" href="@getallpath.getpath/Home"><img src="@getallpath.getpath/images/logo.png" alt="" /></a>
                </div>
                <div id="navbar" class="pull-right header-right navbar-collapse collapse">
                    <nav>
                   
                        <ul>
                            <li><a href="@getallpath.getpath/Home">Home</a></li>
                            <li><a href="@getallpath.getpath/AboutUs">About Us</a></li>
                            @if (Session["TenantId"] == null)
                            {
                                <li><a href="@getallpath.getpath/Plans">Plans &amp; Pricing</a></li>
                                <li><a href="@getallpath.getpath/Tour">Tour</a></li>
                                <li><a href="@getallpath.getpath/bloghome">Blog</a></li>

                            }
                            <li><a href="@getallpath.getpath/Search">Search Properties</a></li>

                            <li><a href="@getallpath.getpath/Contactus">Contact</a></li>

                            @if (Session["TenantId"] != null)
                            {
                                <li><a href="@getallpath.getpath/Tenant/TenantHome">View Profle</a></li>
                                <li><a href="@getallpath.getpath/Tenant/TenantLogout">Logout</a></li>
                            }
                            else
                            {
                                <li><a href="@getallpath.getpath/Login">Log in</a></li>
                            }
                        </ul>
                    </nav>

                 

                </div>
            </div>
        </header>
    </div>
    <div class="clearfix"></div>
    <div class="body-content">
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")


    @RenderSection("scripts", required: false)


    <!-- Footer section -->
