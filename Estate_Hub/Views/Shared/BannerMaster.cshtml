    <script src="http://code.jquery.com/ui/1.11.4/jquery-ui.js"></script>

    <script src="@getallpath.getpath/Scripts/responsive-tabs.js"></script>
    <script src="~/Scripts/select2/select2.min.js"></script>
    <link href="~/Scripts/select2/select2.min.css" rel="stylesheet" />
    <script src="~/Scripts/passwordscheck.js"></script>
    <link href="~/css/passwordscheck.css" rel="stylesheet" />
    <script type="text/javascript">

        $('ul.nav.nav-tabs  a').click(function (e) {
            e.preventDefault();
            $(this).tab('show');
        });

        (function ($) {
            // Test for making sure event are maintained
            $('.js-alert-test').click(function () {
                alert('Button Clicked: Event was maintained');
            });
            fakewaffle.responsiveTabs(['xs', 'sm']);
        })(jQuery);

        $(document).ready(function () {
            if ($('#myModal').hasClass('modalErrorMsg')) {
                $('#myModal').removeClass('hide');
                $('#myModal').modal('show');
            }
          
        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('.select2').select2();
            var optionValue = $('#Country').val();
            if (optionValue == 1) {
                $('#uk1').show();
                $('#ukCanadaUSA1').show();
                $('#ukCanadaAus1').show();
                $("#ukcnt1").show();
                $("#ausCanadaUs1").hide();

                $("#ausUs1").hide();
                $("#us1").hide();
                $("#aus1").hide();
                $("#canada1").hide();

            }
            else if (optionValue == '2') {
                $('#aus1').show();
                $('#ausUs1').show();
                $('#ausCanadaUs1').show();
                $('#ukCanadaAus1').show();


                $("#ukCanadaUSA1").hide();

                $("#us1").hide();

                $("#canada1").hide();
                $("#uk1").hide();
            }
            else if (optionValue == '3') {
                $('#us1').show();
                $('#ausUs1').show();
                $('#ukCanadaUSA1').show();
                $('#ausCanadaUs1').show();
                $("#ukCanadaAus1").hide();
                $("#aus1").hide();
                $("#canada1").hide();
                $("#uk1").hide();
            }
            else if (optionValue == '4') {
                $('#canada1').show();
                $('#ukCanadaUSA1').show();
                $('#ausCanadaUs1').show();
                $('#ukCanadaAus1').show();



                $("#ausUs1").hide();
                $("#us1").hide();
                $("#aus1").hide();

                $("#uk1").hide();
            }
                $("#Country").change(function () {
                    $("#State").empty();
                    $.ajax({
                        type: 'POST',
                        url: '@Url.Action("getstates")',

                        dataType: 'json',

                        data: { id: $("#Country").val() },


                        success: function (states) {


                            $.each(states, function (i, state) {
                                $("#State").append('<option value="' + state.Value + '">' +
                                     state.Text + '</option>');

                            });
                        },
                        error: function (ex) {
                          //  alert('Failed to retrieve states.' + ex);
                        }
                    });                  
                 

                    return false;
                })
            });
    </script>
