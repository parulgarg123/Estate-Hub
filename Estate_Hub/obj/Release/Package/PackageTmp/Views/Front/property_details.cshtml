                                                { @Html.Display("-") }
                                            </td>
                                            <td>
                                                @if (wrk.Notes != null)
                                                {   @Html.DisplayName(wrk.Notes) }
                                                else
                                                {  @Html.Display("-") }
                                            </td>

                                            <td>
                                                @Html.DisplayName(wrk.FaultType.FaultTypeName)
                                            </td>
                                            <td>
                                                @if (wrk.Priority == 1)
                                                {
                                                    <span>Low</span>
                                                }
                                                else if (wrk.Priority == 2)
                                                { <span>Medium</span>

                                                }
                                                else if (wrk.Priority == 3)
                                                {
                                                    <span>High</span>

                                                }
                                            </td>
                                            <td>
                                                Pending
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            }
                            else
                            {
                                <table width="100%" class="table_list" cellspacing="0">
                                    <caption>
                                        <h6 style="color: white; text-align: center"><b>Pending Issues for this Property</b></h6>
                                    </caption>
                                    <thead>

                                    <th>Work Order#</th>
                                    <th>Due</th>
                                    <th>Summery</th>

                                    <th>Type</th>
                                    <th>Priority</th>

                                    <th>Opened</th>

                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td colspan="7">
                                                <hr />
