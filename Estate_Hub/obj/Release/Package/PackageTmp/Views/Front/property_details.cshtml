                <div class="tab-pane" id="tab-2">

                    <div class="columns clear">
                        <div class="top_info">
                            <h3>Work Orders</h3>
                        </div>
                        @foreach (var wrk in ViewBag.workorders as List<Proptiwise.Models.Fault>)
                        {
                            if (wrk.JobStatus == 0)
                            {
                                <table width="100%" class="table_list" cellspacing="0">
                                    <caption>
                                        <h6 style="color: white; text-align: center"><b>Open Issues for this Property</b></h6>
                                    </caption>
                                    <thead>

                                    <th>Work Order#</th>
                                    <th>Due</th>
                                    <th>Summery</th>

                                    <th>Type</th>
                                    <th>Priority</th>

                                    <th>Opened</th>

                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>
                                                @Html.DisplayName(wrk.FaultDescription)
                                            </td>
                                            <td>
                                                @if (wrk.MaintenanceCost != null)
                                                {
                                                    @Html.DisplayName(wrk.MaintenanceCost.ToString())}
                                                else if (wrk.TenantMaintainanceCost != null)
                                                { @Html.DisplayName(wrk.TenantMaintainanceCost.ToString())  }
                                                else
                                                { @Html.Display("-") }
                                            </td>
                                            <td>
                                                @if (wrk.Notes != null)
                                                {   @Html.DisplayName(wrk.Notes) }
                                                else
                                                {  @Html.Display("-") }
                                            </td>

                                            <td>
                                                @Html.DisplayName(wrk.FaultType.FaultTypeName)
                                            </td>
                                            <td>
                                                @if (wrk.Priority == 1)
                                                {
                                                    <span>Low</span>
                                                }
                                                else if (wrk.Priority == 2)
                                                { <span>Medium</span>

                                                }
                                                else if (wrk.Priority == 3)
                                                {
                                                    <span>High</span>

                                                }
                                            </td>
                                            <td>
                                                Opened
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            }
                            else
                            {
                                <table width="100%" class="table_list" cellspacing="0">
                                    <caption>
                                        <h6 style="color: white; text-align: center"><b>Open Issues for this Property</b></h6>
                                    </caption>
                                    <thead>

                                    <th>Work Order#</th>
                                    <th>Due</th>
                                    <th>Summery</th>
                                    <th>More</th>
                                    <th>Type</th>
                                    <th>Priority</th>

                                    <th>Opened</th>

                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td colspan="7">
                                                <hr />
                                                <p style="text-align: center">No Open Issues</p>
                                                <hr />
                                            </td>
                                        </tr>


                                    </tbody>
                                </table>
                            }
