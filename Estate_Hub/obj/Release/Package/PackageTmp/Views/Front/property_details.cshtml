                                <table width="100%" class="table_list" cellspacing="0">
                                    <caption>
                                        <h6 style="color: white; text-align: center"><b>Closed Issues for this Property</b></h6>
                                    </caption>
                                    <thead>
                                    <th>Work Order#</th>
                                    <th>Due</th>
                                    <th>Summery</th>

                                    <th>Type</th>
                                    <th>Priority</th>

                                    <th>Opened</th>

                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>
                                                @Html.DisplayName(wrk.FaultDescription)
                                            </td>
                                            <td>
                                                @if (wrk.MaintenanceCost != null)
                                                {
                                                    @Html.DisplayName(wrk.MaintenanceCost.ToString())}
                                                else if (wrk.TenantMaintainanceCost != null)
                                                { @Html.DisplayName(wrk.TenantMaintainanceCost.ToString())  }
                                                else
                                                { @Html.Display("-") }
                                            </td>
                                            <td>
                                                @if (wrk.Notes != null)
                                                {   @Html.DisplayName(wrk.Notes) }
                                                else
                                                {  @Html.Display("-") }
                                            </td>

                                            <td>
                                                @Html.DisplayName(wrk.FaultType.FaultTypeName)
                                            </td>
                                            <td>
                                                @if (wrk.Priority == 1)
                                                {
                                                    <span>Low</span>
                                                }
                                                else if (wrk.Priority == 2)
                                                { <span>Medium</span>

                                                }
                                                else if (wrk.Priority == 3)
                                                {
                                                    <span>High</span>

                                                }
                                            </td>
                                            <td>
                                                Closed
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            }
                            else
                            {
                                <table width="100%" class="table_list" cellspacing="0">
                                    <caption>
                                        <h6 style="color: white; text-align: center"><b>Closed Issues for this Property</b></h6>
                                    </caption>
                                    <thead>
                                    <th>Work Order#</th>
                                    <th>Due</th>
                                    <th>Summery</th>

                                    <th>Type</th>
                                    <th>Priority</th>

                                    <th>Opened</th>

                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td colspan="7">
                                                <hr />
                                                <p style="text-align: center">No Closed Issues</p>
                                                <hr />
                                            </td>
                                        </tr>


                                    </tbody>
                                </table>
                            }
                        }
                    </div>

                </div>
                <div class="tab-pane" id="tab-3">
                    <div class="columns clear">
                        <div class="top_info">
                            <h3>Inventory</h3>
                        </div>
                        <table width="100%" class="table_list" cellspacing="0">
                            <caption>
                                <h6 style="color: white; text-align: center"><b>Add your inventory, electronics, furniture, or other capital equipment here.</b></h6>
                            </caption>
                            <thead>

                            <th>Name</th>
                            <th>Area/Room</th>
                            <th>Type</th>
                            <th>Quantity</th>
                            <th>Model#</th>
                            <th>Serial#</th>

                            <th>Cost</th>

                            </thead>
                            <tbody>
                                @if (ViewBag.inventory != null)
                                {
                                    var invdata = ViewBag.inventory as List<tbl_inventory>;
                                    if (invdata.Count > 0)
                                    {
                                        foreach (var inv in invdata)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DisplayName(inv.Name)
                                                </td>
                                                <td>
                                                    @Html.DisplayName(inv.tbl_area.AreaName)
                                                </td>
                                                <td>
                                                    @Html.DisplayName(inv.inventory_type.InventoryTypeName)
                                                </td>
                                                <td>
                                                    @Html.DisplayName(inv.Quantity.ToString())
                                                </td>
                                                <td>
                                                    @Html.DisplayName(inv.Modal.ToString())
                                                </td>
                                                <td>
                                                    @Html.DisplayName(inv.Serial.ToString())
                                                </td>
                                                <td>
                                                    @Html.DisplayName(inv.Cost.ToString())
                                                </td>
                                            </tr>
                                        }
                                    }
                                }
                                else
                                {
                                    <tr>
                                        <td colspan="7">
                                            <hr>
                                            <p style="text-align: center">No inventory items</p>
                                            <hr>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>


                    </div>
