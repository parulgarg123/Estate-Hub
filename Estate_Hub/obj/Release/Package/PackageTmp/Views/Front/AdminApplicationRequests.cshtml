                                    @if (data.DepositReq != null)
                                        {
                                        <p>Deposit: @data.DepositReq</p>
                                        }
                                        else
                                        {
                                        <h4>No Deposit</h4>
                                        }
                                    @if (data.Price == null)
                                        {
                                        <p>No Price</p>
                                        }
                                        else
                                        {
                                        <p>Total Price: @data.Price</p>

                                        }

                                </span>
                            </div>
                        </div>

                    </div>
                </td>
            </tr>

