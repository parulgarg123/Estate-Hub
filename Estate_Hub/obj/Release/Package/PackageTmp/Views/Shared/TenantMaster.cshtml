    <script>
        $('.toggle_info').hide();
        $('.up_down').click(function (e) {
            e.stopPropagation();
            $(this).parents('tr').next('.toggle_info').toggle('slow');
        });

        $('.up_downmsg').click(function (e) {
            e.stopPropagation();
            $('.toggle_infomsg').toggle('slow');
        });

        $('.admin_link a').click(function (e) {
            e.stopPropagation();
            $(this).next('.admin_info').toggle('slow');
        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#Noage18").change(function () {
                var selVal = $(this).val();
                $("#textboxDiv").html('');
                if (selVal > 0) {
                    for (var i = 1; i <= selVal; i++) {
                        $("#textboxDiv").append('<input type="text" name="textVal[]" value="" />');


                    }
                }
            });
        });
    </script>
    <script>
        $(document).ready(function () {
            $("#Noage18").change(function () {
                var i = $("#Noage18 :selected").val();
                var str = "";
                for (var j = 1; j <= i; j++) {
                    var id = "txtCustomer" + j;
                    //Remember to add name attribute to get values at server side
                    str = str + "<span>Member " + j + " Full Name: </span><input type='text' id='" + id + "' name='" + id + "'/><br/>";
                }
                $("#content").html(str);
            });
           
            $('.note_btn').click(function () {
                $(this).parent().next('.note_txt').toggle('slow');
            });
            $('.note_txtEdit').hide();
            $('span.note_btnEdit').click(function () {
                $.next().toggle('slow');
            });
            $('.note_btnCancel').click(function () {
                $.parents('div.note_txtEdit').toggle('slow');
            });
        });
    </script>

    <script>
        $('.others').change(function (e) {
            if ($(this).val() == "0") {
                alert("heello");
                $('#addserviceprovider').show();
            }
        });

        $('#addserviceprovider').click(function (e) {
            $('#frmserviceprovider').toggle('slow');
