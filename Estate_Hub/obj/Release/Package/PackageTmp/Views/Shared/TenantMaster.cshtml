    </script>

    <script type="text/jscript">
        $(function () {
            $('#services').change(function () {

                $.getJSON('@getallpath.getpath/ProvidersList/' + $('#services').val(), function (data) {
                    var items = '<option>Select Provider</option>';
                    $.each(data, function (i, providers) {
                        console.log(data);
                        items += "<option value='" + providers.Value + "'>" + providers.Text + "</option>";
                    });

                    $('#serviceproviders').html(items);
                });

            });

        });
    </script>

    <script>
        $(function () {

