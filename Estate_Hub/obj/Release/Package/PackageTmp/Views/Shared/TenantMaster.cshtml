    <script type="text/javascript">

        $('ul.nav.nav-tabs  a').click(function (e) {
            e.preventDefault();
            $(this).tab('show');
        });

        (function ($) {
            // Test for making sure event are maintained
            $('.js-alert-test').click(function () {
                alert('Button Clicked: Event was maintained');
            });
            fakewaffle.responsiveTabs(['xs', 'sm']);
        })(jQuery);

    </script>

    <script type="text/javascript">

        $(document).ready(function () {
            if ($('#myModal').hasClass('modalErrorMsg')) {
                $('#myModal').removeClass('hide');
                $('#myModal').modal('show');
            }

        });
        var optionValue = $('#Country').val();
        if (optionValue == 1) {
            $('.uk11').show();
            $('.ukCanadaUSA11').show();
            $('.ukCanadaAus11').show();

            $(".ausCanadaUs11").hide();

            $(".ausUs11").hide();
            $(".us11").hide();
            $(".aus11").hide();
            $(".canada11").hide();

        }
        else if (optionValue == '2') {
            $('.aus11').show();
            $('.ausUs11').show();
            $('.ausCanadaUs11').show();
            $('.ukCanadaAus11').show();


            $(".ukCanadaUSA11").hide();

            $(".us11").hide();

            $(".canada11").hide();
            $(".uk11").hide();
        }
        else if (optionValue == '3') {
            $('.us11').show();
            $('.ausUs11').show();
            $('.ukCanadaUSA11').show();
            $('.ausCanadaUs11').show();
            $(".ukCanadaAus11").hide();
            $(".aus11").hide();
            $(".canada11").hide();
            $(".uk11").hide();
        }
        else if (optionValue == '4') {
            $('.canada11').show();
            $('.ukCanadaUSA11').show();
            $('.ausCanadaUs11').show();
            $('.ukCanadaAus11').show();
            $(".ausUs11").hide();
            $(".us11").hide();
            $(".aus11").hide();
            $(".uk11").hide();
        }
    </script>
    <div id='myModal' class='modal fade in'>
        <div class="modal-dialog">
            <div class="modal-content">
                <div id='myModalContent'></div>
            </div>
        </div>
    </div>
